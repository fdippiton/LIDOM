@model IEnumerable<LIDOM_MVC.Models.Posicione>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PosNombre)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PosNombre)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.PosId }) |
                    @Html.ActionLink("Details", "Details", new { id=item.PosId }) |
                    @Html.ActionLink("Delete", "Delete", new { id=item.PosId })
            </td>
        </tr>
}
    </tbody>
</table>
