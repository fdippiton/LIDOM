    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TemId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TemId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TemNombre)
        </dt>
